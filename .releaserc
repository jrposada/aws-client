{
    "branches": [
        "main"
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "./src-tauri/target/release/bundle/dmg/*.dmg",
                        "label": "Mac OS"
                    },
                    {
                        "path": "./src-tauri/target/release/bundle/msi/*.msi",
                        "label": "Windows"
                    }
                ]
            }
        ]
    ]
}
